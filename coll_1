using System;
using System.Collections.Generic;

class Program {
    static void Main(string[] args) {
        var lis = new List<char>(){'Q', 'Q', 'Q', 'O', 'Q', 'Q', 'Q', 'Q', 'Q', 'Q'};
        for(int i = 0; i < lis.Count; i++)
        {
          if(lis[i] == 'O') Console.WriteLine(i);
        }
    }
}
